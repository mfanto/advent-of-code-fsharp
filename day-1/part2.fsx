// just to make things easier to read
let basement = -1;

let getDirection c =
    match c with
    | '(' -> 1
    | ')' -> -1
    | _ -> raise (System.ArgumentException("Invalid character"))

let go =
    let rec move directions currentFloor stepNumber =
        if currentFloor = basement then stepNumber
        else match directions with
             | [] -> currentFloor
             | nextStep :: remaining -> move remaining (currentFloor + getDirection nextStep) stepNumber+1
    move input 0 0

printfn "Basement found at step %d" go